#gtk 4.x support
#align the stuff in common with gtk 3.16 and 3.20, skip the stuff that is different
#
alignfiles widgets.jos.css ../gtk-3.20/widgets.jos.css
vimdiff widgets.jos.css ../gtk-3.20/widgets.jos.css
vim widgets.jos.css
vim -O ../gtk-2.0/gtkrc widgets.jos.css 

vim -O widgets.jos.css ../gtk-3.20/widgets.jos.css

./process.py
xfce4-panel -r
xfconf-query -c xsettings -p /Net/ThemeName -s cdetheme
xfconf-query -c xsettings -p /Net/ThemeName -s cdetheme1

